# Staging example (no secrets)
ENV=staging
CORS_ORIGINS=https://staging.jargonless.ai

# Optional host ports if you ever map them (Tunnel does not need them)
HOST_HTTP_PORT=8081
HOST_API_PORT=8001

# App flags
PORTFOLIO_ENV=staging
NODE_ENV=production

# Image tags pushed by the staging workflow
WEB_IMAGE_TAG=staging
API_IMAGE_TAG=staging
